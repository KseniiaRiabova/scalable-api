{
  "name": "scalable-api",
  "version": "1.0.0",
  "scripts": {
    "start:basic": "node dist/basic-api.js",
    "start:redis": "node dist/redis-api.js",
    "start:cluster": "node dist/cluster-api.js",
    "dev": "nodemon --exec \"ts-node src/redis-api.ts\"",
    "dev:basic": "cross-env PORT=3002 nodemon --exec \"ts-node src/basic-api.ts\"",
    "dev:redis": "cross-env PORT=3000 nodemon --exec \"ts-node src/redis-api.ts\"",
    "dev:cluster": "cross-env PORT=3001 nodemon --exec \"ts-node src/cluster-api.ts\"",
    "start-all": "concurrently \"npm run dev:basic\" \"npm run dev:redis\" \"npm run dev:cluster\"",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "rate-limit-redis": "^4.2.2",
    "redis": "^5.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
